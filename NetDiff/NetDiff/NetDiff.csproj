<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>This is the NetDiff package built for .NET Standard rather than .NET Framework so that it can target multiple platforms.</Description>
    <PackageProjectUrl>https://github.com/davepruitt/NetDiff</PackageProjectUrl>
    <RepositoryUrl>https://github.com/davepruitt/NetDiff</RepositoryUrl>
    <PackageTags>diff, netdiff</PackageTags>
    <PackageReleaseNotes>This is the NetDiff package built for .NET Standard rather than .NET Framework so that it can target multiple platforms.</PackageReleaseNotes>
    <PackageId>NetDiffStandard</PackageId>
    <Authors>David Pruitt and skanmera</Authors>
    <PackageLicenseExpression></PackageLicenseExpression>
    <Version>1.0.0</Version>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
